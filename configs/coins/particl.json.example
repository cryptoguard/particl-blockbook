        "backend_message_queue": 51736,
        "blockbook_internal": 9135,
        "blockbook_public": 9235
    },
    "ipc": {
        "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
        "rpc_user": "particlrpc",
        "rpc_pass": "CHANGE_THIS_TO_YOUR_RPC_PASSWORD",
        "rpc_timeout": 25,
        "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
    },
    "backend": {
        "package_name": "backend-particl",
        "package_revision": "satoshilabs-1",
        "system_user": "particl",
        "version": "27.99.1.0",
        "binary_url": "",
        "verification_type": "none",
        "verification_source": "",
        "extract_command": "",
        "exclude_files": [],
        "exec_command_template": "/usr/local/bin/particld -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf=/home/mint/.particl/particl.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
        "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/debug.log",
        "postinst_script_template": "",
        "service_type": "forking",
        "service_additional_params_template": "",
        "protect_memory": true,
        "mainnet": true,
        "server_config_file": "",
        "client_config_file": "",
        "additional_params": {}
    },
    "blockbook": {
        "package_name": "blockbook-particl",
        "system_user": "blockbook-particl",
        "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
        "public_binding_template": ":{{.Ports.BlockbookPublic}}",
        "explorer_url": "",
        "additional_params": "",
